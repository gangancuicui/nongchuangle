.con {
  position: relative;
}
swiper {
  height: 340;
  border-bottom: 5rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
  border-bottom: 5rpx;
}
.nav-list {
  width: 100%;
  border-top: 20rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 20rpx;
}
.nav-item .cate {
  width: 50%;
  height: 100rpx;
  background-color: #9bffc3;
  float: left;
  border-top-left-radius: 46rpx;
  border-bottom-left-radius: 46rpx;
}
.nav-item .cate .text {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav-item .weather {
  width: 50%;
  height: 100rpx;
  background-color: #00ffff;
  float: right;
  border-top-right-radius: 46rpx;
  border-bottom-right-radius: 46rpx;
}
.nav-item .weather .text {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.temai image {
  height: 100rpx;
  width: 200rpx;
}
.lists {
  display: flex;
  flex-wrap: wrap;
}
.list {
  width: 40%;
  height: 320px;
  padding: 10rpx;
  border: 2rpx solid #e5e5e5;
  border-radius: 30rpx;
  position: relative;
  margin: 0 20rpx 0 20rpx;
  margin-bottom: 20rpx;
}
.imgg {
  height: 50%;
  overflow: hidden;
}
.image {
  width: 50%;
  display: block;
}
.price {
  color: red;
}
.price text {
  font-size: 24rpx;
  color: #000000;
}
.name {
  color: #666;
}
.add {
  background: orange;
  color: #fff;
  border: none;
  padding: 5px 10px;
  border-radius: 5px;
  position: absolute;
  bottom: 10rpx;
  right: 10px;
  width: 200rpx;
  height: 50rpx;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.search {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}

